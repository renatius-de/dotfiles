# source default configuration
[[ -r /etc/zsh/zlogin ]] && source /etc/zsh/zlogin || true
[[ -r /etc/zlogin ]]     && source /etc/zlogin     || true

# Load RVM into a shell session *as a function*
[[ -s ~/.rvm/scripts/rvm ]] && source ~/.rvm/scripts/rvm || true

# vim: filetype=zsh textwidth=80 foldmethod=marker
