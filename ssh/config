Match host="github.*,gitlab.*,bitbucket.*"
    IdentitiesOnly yes
    PasswordAuthentication no
    User git

include config.local

Host *
    CheckHostIP yes
    Compression yes
    EnableSSHKeysign yes
    ExitOnForwardFailure yes
    HashKnownHosts yes
    IdentitiesOnly yes
    PubKeyAuthentication yes
    StrictHostKeyChecking yes
    UseKeychain yes
